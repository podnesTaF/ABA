{
  "kind": "collectionType",
  "collectionName": "contact_requests",
  "info": {
    "singularName": "contact-request",
    "pluralName": "contact-requests",
    "displayName": "ContactRequest"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "enumeration",
      "enum": [
        "events",
        "partnership",
        "support"
      ],
      "required": true
    },
    "message": {
      "type": "text",
      "required": true,
      "minLength": 10
    }
  }
}
